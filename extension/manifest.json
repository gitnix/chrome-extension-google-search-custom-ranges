{
	"name": "Google Search Quick Custom Ranges",
	"version": "1.0.2",
	"description": "Grants ability to add additional quick date ranges in google search.",
	"icons": {
		"16": "icons/icon-16.png",
		"48": "icons/icon-48.png",
		"128": "icons/icon-128.png"
	},
	"manifest_version": 2,
	"permissions": ["storage"],
	"options_page": "options.html",
	"content_scripts": [
		{
			"matches": ["https://www.google.com/search*"],
			"js": ["searchRanges.js"]
		}
	]
}
